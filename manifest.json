
{
    "name": "dev-search",
    "description": "Search prefixes for lazy developers, type ds + Tab in your search box and watch the magic.",
    "version": "1.2",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "omnibox": { "keyword" : "ds" },
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "newtab_search16.png",
        "32": "newtab_search32.png"
        }
    },
    "icons": {
      "16": "newtab_search16.png",
      "32": "newtab_search32.png",
      "48": "newtab_search48.png",
      "128": "newtab_search128.png"
    },
    "permissions": [
        "storage"
    ]
  }
  